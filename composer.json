{
  "name": "transip/tipctl",
  "description": "TransIP REST API CLI client",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "TransIP",
      "email": "support@transip.eu"
    }
  ],
  "config": {
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.2.5|^8.0",
    "ext-json": "*",
    "symfony/console": "^5.4",
    "symfony/finder": "^5.4",
    "symfony/filesystem": "^5.4",
    "transip/transip-api-php": "^6.48",
    "symfony/yaml": "^5.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "php-parallel-lint/php-parallel-lint": "^1.2",
    "phpstan/phpstan": "^1.5",
    "rector/rector": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Transip\\Api\\CLI\\": "src"
    }
  },
  "bin": [
    "bin/tipctl"
  ]
}
